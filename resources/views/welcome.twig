<script src="{{ url('vendor/jquery.mousewheel.min.js') }}"></script>
<!-- /jQuery Mousewheel -->
<!-- jQuery Mousewheel Smoothscroll -->
<script src="{{ url('vendor/jquery.nicescroll.min.js') }}"></script>
<script src="{{ url('vendor/jquery.nicescroll.plus.js') }}"></script>
<!-- /jQuery Mousewheel Smoothscroll -->
<!-- Waypoints (for CSS3 animations) -->
<script src="{{ url('vendor/waypoints.min.js') }}"></script>
<!-- /waypoints -->
<!-- Modal box-->
<script src="{{ url('vendor/nivo-lightbox/nivo-lightbox.min.js') }}"></script>
<!-- /Modal Box -->
<!-- Carousel-->
<script src="{{ url('vendor/jquery.bxslider.min.js') }}"></script>
<!-- /Carousel -->
<!-- Front-end Validator (for forms) -->
<script src="{{ url('vendor/jquery.validate.min.js') }}"></script>
<!-- /Front-end Validator -->
<!-- placeholder Support for IE -->
<script src="{{ url('vendor/placeholders.jquery.min.js') }}"></script>
<!-- /placeholder support -->
<!-- Cross-browser -->
<script src="{{ url('vendor/cross-browser.js') }}"></script>
<!-- /Cross-browser -->
<!-- Configurations -->
<script src="{{  url('js/main.js') }}"></script>
<script src="{{ mix('/js/app.js') }}"></script>
<!-- /Configurations -->
</body>
</html>