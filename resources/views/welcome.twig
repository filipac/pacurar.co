<script src="{{ url('vendor/jquery-1.11.1.min.js') }}"></script>
<!-- /jquery -->
<!-- bootstrap -->
<script src="{{ url('vendor/bootstrap/js/bootstrap.min.js') }}"></script>
<!-- /bootstrap -->
<!-- easing -->
<script src="{{ url('vendor/jquery.easing.min.js') }}"></script>
<!-- /easing -->
